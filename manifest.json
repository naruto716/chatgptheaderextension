{
  "name": "Request Modifier Extension",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Use declarativeNetRequest to modify request headers.",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback",
    "storage"
  ],
  "host_permissions": [
    "https://chatgpt.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}